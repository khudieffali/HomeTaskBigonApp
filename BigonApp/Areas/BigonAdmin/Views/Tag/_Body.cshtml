@model IEnumerable<Tag>

@foreach (var item in Model)
{
    <tr>
        <td>@item.Name</td>
        <td>
            <a asp-controller="tag" asp-action="edit" asp-route-id="@item.Id" class="btn btn-warning text-white"><i class="fa-solid fa-pen-to-square"></i></a>
            <a asp-controller="tag" asp-action="details" asp-route-id="@item.Id" class="btn btn-primary text-white"><i class="fa-solid fa-info"></i></a>
            <a onclick="removeTag(@item.Id,'@item.Name')" class="btn btn-danger text-white">
                <i class="fa-solid fa-trash"></i>
            </a>
        </td>
    </tr>

}